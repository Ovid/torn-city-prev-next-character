{
  "manifest_version": 2,

  "name": "prev/next in Torn",
  "description": "Easily search for old players in Torn.com",
  "version": "1.0",

  "permissions": [
    "https://www.torn.com/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
